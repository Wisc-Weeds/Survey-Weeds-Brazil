@article{mischler_use_2010,
  title = {Use of a {{Rolled}}-Rye {{Cover Crop}} for {{Weed Suppression}} in {{No}}-{{Till Soybeans}}},
  author = {Mischler, Ruth A. and Curran, William S. and Duiker, Sjoerd W. and Hyde, Jeffrey A.},
  date = {2010-09},
  journaltitle = {Weed Technology},
  volume = {24},
