@article{werle_independent_2017,
  title = {Independent {{Evolution}} of {{Acetolactate Synthase}}–Inhibiting {{Herbicide Resistance}} in {{Weedy Sorghum Populations}} across {{Common Geographic Regions}}},
  author = {Werle, Rodrigo and Begcy, Kevin and Yerka, Melinda K. and Mower, Jeffrey P. and Dweikat, Ismail and Jhala, Amit J. and Lindquist, John L.},
